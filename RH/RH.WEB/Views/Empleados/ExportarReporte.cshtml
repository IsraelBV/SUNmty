
@{
    ViewBag.Title = "Empleados";
    ViewBag.EXTRA = "Exportar Reporte";
    Layout = "~/Views/Shared/_MenuSecundario.cshtml";
}

<link rel="stylesheet" href="~/Content/Empleados/ImportacionMasiva.css" />
<link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/t/bs/pdfmake-0.1.18,dt-1.10.11,b-1.1.2,b-html5-1.1.2,b-print-1.1.2,kt-2.1.1,r-2.0.2/datatables.min.css" />

<div class="panel panel-default">
    <div class="panel-heading">
        <div class="col-md-3">
            <a href="@Url.Action("DescargarReporte", "Empleados")" class="btn btn-default btnPanel"><i class="fa fa-file-excel-o"></i> Descargar Reporte</a>
        </div>
        <div class="col-md-6">
            <div class="form-group">
                <input type="file" class="form-control" accept=".xlsx" id="fileInput" name="file" required disabled/>
            </div>
        </div>
        <div class="col-md-3 text-right">
            <a href="@Url.Action("UploadRecords", "Empleados")" class="btn btn-default btnPanel" id="btnProcesar" disabled><i class="fa fa-download"></i> Procesar</a>
        </div>
    </div>
    <div class="panel-body">
        <div id="records">
            <div class="progress">
                <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%"></div>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript" src="https://cdn.datatables.net/t/bs/pdfmake-0.1.18,dt-1.10.11,b-1.1.2,b-html5-1.1.2,b-print-1.1.2,kt-2.1.1,r-2.0.2/datatables.min.js"></script>


